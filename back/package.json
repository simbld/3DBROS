{
  "name": "ecommerce-prisma-nestjs",
  "version": "1.0.0",
  "description": "",
  "main": "back/src/main.ts",
  "scripts": {
    "test": "jest",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name migration --schema=prisma/schema.prisma",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed --preview-feature",
    "prisma:drop": "npx prisma migrate reset --preview-feature && npx prisma db push --preview-feature",
    "prisma:push": "npx prisma db push --preview-feature",
    "prisma:pull": "npx prisma db pull --preview-feature",
    "prisma:status": "npx prisma migrate status --preview-feature",
    "prisma:deploy": "npx prisma migrate deploy --preview-feature",
    "prisma:full": "npm run prisma:migrate && npm run prisma:generate"
  },
  "author": "simbld",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.4.4",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/platform-socket.io": "^10.4.4",
    "@nestjs/websockets": "^10.4.4",
    "@prisma/client": "^5.20.0",
    "@prisma/extension-accelerate": "^1.2.1",
    "aws-sdk": "^2.1546.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.14.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prisma": "^5.20.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.4",
    "@types/aws-sdk": "^0.0.42",
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-morph": "^21.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  }
}

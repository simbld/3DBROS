{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "target": "es2021",
    "baseUrl": "./",
    "paths": {
      "@common/*": ["src/common/*"],
      "@config/*": ["src/config/*"],
      "@controllers/*": ["src/controllers/*"],
      "@enums/*": ["src/enums/*"],
      "@env/*": ["env/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@middlewares/*": ["src/middlewares/*"],
      "@models/*": ["src/models/*"],
      "@pipes/*": ["src/pipes/*"],
      "@prisma/*": ["prisma/*"],
      "@services/*": ["src/services/*"],
      "@shared/*": ["src/shared/*"],
      "@test/*": ["src/test/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"],
      "@/*": ["src/*"],

      // models path
      "@entityCart/*": ["src/data/cart/*"],
      "@entityCartItem/*": ["src/data/cartItem/*"],
      "@entityCategory/*": ["src/data/category/*"],
      "@entityCollection/*": ["src/data/collection/*"],
      "@entityDelivery/*": ["src/data/delivery/*"],
      "@entityNotification/*": ["src/data/notification/*"],
      "@entityOrder/*": ["src/data/order/*"],
      "@entityOrderItem/*": ["src/data/orderItem/*"],
      "@entityPayment/*": ["src/data/payment/*"],
      "@entityProduct/*": ["src/data/product/*"],
      "@entityProductCollection/*": ["src/data/productCollection/*"],
      "@entityReview/*": ["src/data/review/*"],
      "@entityUser/*": ["src/data/user/*"],
      "@entityVat/*": ["src/data/vat/*"],

      // controllers paths
      "@controllerCart/*": ["src/controllers/cart/*"],
      "@controllerCartItem/*": ["src/controllers/cartItem/*"],
      "@controllerCategory/*": ["src/controllers/category/*"],
      "@controllerCollection/*": ["src/controllers/collection/*"],
      "@controllerDelivery/*": ["src/controllers/delivery/*"],
      "@controllerNotification/*": ["src/controllers/notification/*"],
      "@controllerOrder/*": ["src/controllers/order/*"],
      "@controllerOrderItem/*": ["src/controllers/orderItem/*"],
      "@controllerPayment/*": ["src/controllers/payment/*"],
      "@controllerProduct/*": ["src/controllers/product/*"],
      "@controllerProductCollection/*": ["src/controllers/productCollection/*"],
      "@controllerReview/*": ["src/controllers/review/*"],
      "@controllerUser/*": ["src/controllers/user/*"],
      "@controllerVat/*": ["src/controllers/vat/*"],

      // services paths
      "@serviceCart/*": ["src/services/cart/*"],
      "@serviceCartItem/*": ["src/services/cartItem/*"],
      "@serviceCategory/*": ["src/services/category/*"],
      "@serviceCollection/*": ["src/services/collection/*"],
      "@serviceDelivery/*": ["src/services/delivery/*"],
      "@serviceNotification/*": ["src/services/notification/*"],
      "@serviceOrder/*": ["src/services/order/*"],
      "@serviceOrderItem/*": ["src/services/orderItem/*"],
      "@servicePayment/*": ["src/services/payment/*"],
      "@serviceProduct/*": ["src/services/product/*"],
      "@serviceProductCollection/*": ["src/services/productCollection/*"],
      "@serviceReview/*": ["src/services/review/*"],
      "@serviceUser/*": ["src/services/user/*"],
      "@serviceVat/*": ["src/services/vat/*"],

      // modules paths
      "@moduleCart/*": ["src/modules/cart/*"],
      "@moduleCartItem/*": ["src/modules/cartItem/*"],
      "@moduleCategory/*": ["src/modules/category/*"],
      "@moduleCollection/*": ["src/modules/collection/*"],
      "@moduleDelivery/*": ["src/modules/delivery/*"],
      "@moduleNotification/*": ["src/modules/notification/*"],
      "@moduleOrder/*": ["src/modules/order/*"],
      "@moduleOrderItem/*": ["src/modules/orderItem/*"],
      "@modulePayment/*": ["src/modules/payment/*"],
      "@moduleProduct/*": ["src/modules/product/*"],
      "@moduleProductCollection/*": ["src/modules/productCollection/*"],
      "@moduleReview/*": ["src/modules/review/*"],
      "@moduleUser/*": ["src/modules/user/*"],
      "@moduleVat/*": ["src/modules/vat/*"],

      // resolvers paths
      "@resolverCart/*": ["src/resolvers/cart/*"],
      "@resolverCartItem/*": ["src/resolvers/cartItem/*"],
      "@resolverCategory/*": ["src/resolvers/category/*"],
      "@resolverCollection/*": ["src/resolvers/collection/*"],
      "@resolverDelivery/*": ["src/resolvers/delivery/*"],
      "@resolverNotification/*": ["src/resolvers/notification/*"],
      "@resolverOrder/*": ["src/resolvers/order/*"],
      "@resolverOrderItem/*": ["src/resolvers/orderItem/*"],
      "@resolverPayment/*": ["src/resolvers/payment/*"],
      "@resolverProduct/*": ["src/resolvers/product/*"],
      "@resolverProductCollection/*": ["src/resolvers/productCollection/*"],
      "@resolverReview/*": ["src/resolvers/review/*"],
      "@resolverUser/*": ["src/resolvers/user/*"],
      "@resolverVat/*": ["src/resolvers/vat/*"],

      // middlewares paths
      "@middlewareCart/*": ["src/middlewares/cart/*"],
      "@middlewareCartItem/*": ["src/middlewares/cartItem/*"],
      "@middlewareCategory/*": ["src/middlewares/category/*"],
      "@middlewareCollection/*": ["src/middlewares/collection/*"],
      "@middlewareDelivery/*": ["src/middlewares/delivery/*"],
      "@middlewareNotification/*": ["src/middlewares/notification/*"],
      "@middlewareOrder/*": ["src/middlewares/order/*"],
      "@middlewareOrderItem/*": ["src/middlewares/orderItem/*"],
      "@middlewarePayment/*": ["src/middlewares/payment/*"],
      "@middlewareProduct/*": ["src/middlewares/product/*"],
      "@middlewareProductCollection/*": ["src/middlewares/productCollection/*"],
      "@middlewareReview/*": ["src/middlewares/review/*"],
      "@middlewareUser/*": ["src/middlewares/user/*"],
      "@middlewareVat/*": ["src/middlewares/vat/*"],

      // DTOs paths
      "@dtoCart/*": ["src/models/cart/dto/*"],
      "@dtoCartItem/*": ["src/models/cartItem/dto/*"],
      "@dtoCategory/*": ["src/models/category/dto/*"],
      "@dtoCollection/*": ["src/models/collection/dto/*"],
      "@dtoDelivery/*": ["src/models/delivery/dto/*"],
      "@dtoNotification/*": ["src/models/notification/dto/*"],
      "@dtoOrder/*": ["src/models/order/dto/*"],
      "@dtoOrderItem/*": ["src/models/orderItem/dto/*"],
      "@dtoPayment/*": ["src/models/payment/dto/*"],
      "@dtoProduct/*": ["src/models/product/dto/*"],
      "@dtoProductCollection/*": ["src/models/productCollection/dto/*"],
      "@dtoReview/*": ["src/models/review/dto/*"],
      "@dtoUser/*": ["src/models/user/dto/*"],
      "@dtoVat/*": ["src/models/vat/dto/*"]
    },
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "outDir": "./dist",
    "lib": ["es2021", "dom"],
    "typeRoots": ["node_modules/@types", "types"],
    "incremental": true,
    "types": ["jest", "node", "aws-sdk"]
  },
  "include": [
    "src/**/*.ts",
    "./../src/**/*.ts",
    "test/**/*.ts",
    "./../test/**/*.ts",
    "back/test/**/*.ts",
    "./../back/test/**/*.ts",
    "app.e2e-spec.ts",
    "./../app.e2e-spec.ts",
    "prisma/prisma.service.ts",
    "./../prisma/prisma.service.ts",
    "src/enums/*",
    "./../src/graphQl/enums/*",
    "src/shared/**/*",
    "./../src/shared/**/*"
  ],
  "references": [
    {
      "path": ["./tsconfig.build.json"],
      "composite": true
    }
  ],
  "exclude": ["node_modules", "dist"]
}
